#include <stdint.h>

int main(void)
{
  uint32_t *pClkCtrlReg = (uint32_t) 0x40023830;
  uint32_t *pPortModeReg = (uint32_t) 0x40020800;
  uint32_t *pPortOutReg = (uint32_t) 0x40020814;

